# Architectures
CONFIG_ARCH_YUPIK=y
CONFIG_PINCTRL_YUPIK=y
CONFIG_SM_GCC_YUPIK=y
CONFIG_SM_GPUCC_YUPIK=y
CONFIG_SM_VIDEOCC_YUPIK=y
CONFIG_SM_CAMCC_YUPIK=y
CONFIG_SM_DISPCC_YUPIK=y
CONFIG_QCOM_YUPIK_LLCC=y
CONFIG_INTERCONNECT_QCOM_YUPIK=y

# Touchpanel
CONFIG_TOUCHPANEL_ILITEK=y
CONFIG_TOUCHPANEL_ILITEK_ILITEK7807S=y

# WiFi
CONFIG_CNSS_QCA6490=n
CONFIG_CNSS_QCA6750=y

# NFC
CONFIG_NFC_ST21_I2C=y
CONFIG_NFC_ST54_SPI=y
CONFIG_OPLUS_NFC=y

# FocalTech
CONFIG_TOUCHPANEL_FOCAL=y
CONFIG_TOUCHPANEL_FOCAL_FT3518=y
CONFIG_TOUCHPANEL_FOCAL_FT3658U=y

# Fingerprint
# CONFIG_OPLUS_FINGERPRINT_GOODIX_OPTICAL is not set
CONFIG_OPLUS_FINGERPRINT_JIIOV_OPTICAL=y

# Power
CONFIG_OPLUS_CHG=y
CONFIG_OPLUS_SM8350_CHARGER=y
CONFIG_OPLUS_SHORT_C_BATT_CHECK=y
CONFIG_OPLUS_CALL_MODE_SUPPORT=y
CONFIG_OPLUS_SMART_CHARGER_SUPPORT=y
# CONFIG_OPLUS_SHORT_HW_CHECK is not set
CONFIG_OPLUS_CHIP_SOC_NODE=y
# CONFIG_OPLUS_SHORT_IC_CHECK is not set
CONFIG_OPLUS_SHORT_USERSPACE=y
# CONFIG_OPLUS_RTC_DET_SUPPORT is not set
CONFIG_OPLUS_CHECK_CHARGERID_VOLT=y
CONFIG_OPLUS_SHIP_MODE_SUPPORT=y
# CONFIG_OPLUS_CHARGER is not set
# CONFIG_OPLUS_CHARGER_WIRELESS_RA9530 is not set
# CONFIG_OPLUS_DYNAMIC_CONFIG is not set